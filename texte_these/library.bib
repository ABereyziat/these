issn = {0308-0501},
journal = {Fire and Materials},
keywords = {bois,char oxidation,charring,conductivity,feu,natural fires,propri{\'{e}}t{\'{e}}s thermiques du bois,temperature,thermal analysis,timber structures,wood},
mendeley-tags = {bois,feu,propri{\'{e}}t{\'{e}}s thermiques du bois},
month = {jan},
