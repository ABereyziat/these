file = {:C\:/Users/33634/Documents/these/mendeley/Saulnier et al._2020_Wood as Fire Protection of Steel in Hybrid Structural Elements BT - Wood & Fire Safety.pdf:pdf},
isbn = {978-3-030-41235-7},
keywords = {acier-bois,feu,profil{\'{e}} {\'{e}}pais},
mendeley-tags = {acier-bois,feu,profil{\'{e}} {\'{e}}pais},
pages = {420--425},
publisher = {Springer International Publishing},
title = {{Wood as Fire Protection of Steel in Hybrid Structural Elements BT - Wood & Fire Safety}},
