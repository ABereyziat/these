doi = {10.1007/s10694-019-00901-0},
file = {:C\:/Users/33634/Documents/these/mendeley/Quiquero et al._2020_Finite Element Modelling of Post-tensioned Timber Beams at Ambient and Fire Conditions.pdf:pdf},
issn = {1572-8099},
