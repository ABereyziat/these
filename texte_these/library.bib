journal = {Materials},
keywords = {Eurocode 5,charring rate,dowel connection,fire resistance,steel composite,timber},
month = {dec},
number = {23},
pages = {4003},
publisher = {Multidisciplinary Digital Publishing Institute},
title = {{Experimental Assessment of the Fire Resistance Mechanisms of Timber–Steel Composites}},
