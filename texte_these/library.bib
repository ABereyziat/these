journal = {Structures},
keywords = {Cold-formed steel,Composite beam,Cross-laminated timber,Finite element modelling,Modular buildings,profil{\'{e}} mince},
mendeley-tags = {profil{\'{e}} mince},
pages = {681--690},
title = {{Development of cross laminated timber-cold-formed steel composite beam for floor system to sustainable modular building construction}},
