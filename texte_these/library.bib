@phdthesis{Gammon1987,
abstract = {The application of reliability analysis to the fire performance of structural assemblies requires statistical data on both fire severity and fire resistance. Since the standard test for fire resistance of wood-frame wall assemblies is both time-consuming and costly, it would be desirable to have a numerical method that could be used in conjunction with fire tests to generate the data required for reliability analysis. A computer model, based on the finite element method, was developed to simulate the ASTM E-119 fire test of a wall assembly. This transient heat balance also models heat transfer across spaces within the wall assembly, such as a stud cavity. Spaces can exist at the start of the problem or be created and/or merged during program execution. Burnthrough of the wall facings is simulated by allowing elements to be removed from the problem when their temperatures exceed a critical value. The computer program allows basic material property data to be input either from a data file or interactively as the program is executed. The program was designed to generate data consistent with a reliability analysis and thus has the capability of sequentially solving problems for the same mesh, but with different material properties. Excellent agreement with published data was found in simulations of a wood slab exposed to fire. Simulations of uninsulated 2 x 4 wood stud walls, spaced 16" on center and with various wall facings, were in good agreement with the results of published ASTM E-119 wall tests. However, changes in the thickness of the wall facings did not always lead to changes in the fire endurance time that were of the same magnitude as that reported in the literature. This study demonstrated that computer simulations of wood-frame wall assemblies exposed to fire could be used to generate the fire resistance data necessary for reliability analysis. However, more research is needed on the thermal properties of wall materials.},
annote = {Source: Dissertation Abstracts International, Volume: 48-09, Section: B, page: 2522.},
author = {Gammon, Barry Wayne},
file = {:C\:/Users/33634/Documents/these/mendeley//Gammon_1987_Reliability analysis of wood-frame wall assemblies exposed to fire.pdf:pdf},
keywords = {Bois,agriculture,feu,propri{\'{e}}t{\'{e}}s thermiques du bois,wood technology},
mendeley-tags = {Bois,feu,propri{\'{e}}t{\'{e}}s thermiques du bois},
pages = {377},
school = {University of California, Berkley},
title = {{Reliability analysis of wood-frame wall assemblies exposed to fire}},
year = {1987}
}
