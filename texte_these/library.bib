mendeley-tags = {connexion,plancher clt,profil{\'{e}} {\'{e}}pais},
pages = {560--575},
publisher = {Elsevier Ltd},
title = {{Semi-rigid partial-strength steel-timber composite (STC) connections with mechanically anchored steel rods}},
url = {https://doi.org/10.1016/j.jcsr.2019.04.017},
volume = {158},
