journal = {Composite Structures},
keywords = {Bending,Confinement,Fibre reinforced polymer,Softwood timber,Utility pole,confinement},
mendeley-tags = {confinement},
pages = {111807},
title = {{Mechanical performance of fibre reinforced polymer confined softwood timber for pole applications}},
url = {https://www.sciencedirect.com/science/article/pii/S0263822319329381},
volume = {235},
year = {2020}
}
