title = {{The design and installation of a five-story new timber building in Japan}},
url = {http://scholar.google.com/scholar?hl=en&btnG=Search&q=intitle:THE+DESIGN+AND+INSTALLATION+OF+A+FIVE-STORY+NEW+TIMBER+BUILDING+IN+JAPAN#1},
year = {2009}
}
